end
if not is_vip(msg) and not is_creatorbasic(msg) then
if bot_id then
if not is_vip(msg) and not is_creatorbasic(msg) then
local hash = 'flood:'..msg.sender_user_id_..':'..msg.chat_id_..':msg-num'
local msgs = tonumber(database:get(hash) or 0)
if msgs > (floodMax - 1) then
