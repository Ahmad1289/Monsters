function id_by_reply(extra, result, success)
if not database:get('monsters:'..bot_id..'id:mute'..msg.chat_id_) then 
local tahna = (database:get('monsters:'..bot_id..'nummsg'..msg.chat_id_..result.sender_user_id_) or 0)
